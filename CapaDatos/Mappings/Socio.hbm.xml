<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="CapaNegocio" namespace="CapaNegocio">
  <class name="Socio">

    <id name="Id" column="IdSocio">
      <generator class="identity" />
    </id>

    <property name="Nombre" column="Nombre" type="string" />
    <property name="Apellido" column="Apellido" type="string" />
    <property name="NombreUsuario" column="NombreUsuario" type="string" />
    <property name="Estado" column="IdEstadoSocio" type="EstadoSocio, CapaNegocio"></property>


  </class>
</hibernate-mapping>